<h3 class="page-header">
    <i class="fa fa-list"></i> Logs</h3>

<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <i class="fa fa-th-list"></i> User Logs
        </div>

        <div class="panel-body">
            <table class="table table-striped table-bordered table-hover" id="tblLogs" style="width: 100%;">
                <thead>
                    <tr>
                        <th>Email</th>
                        <th>Level</th>
                        <th>Message</th>
                        <th>Date Time</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each logList}}
                    <tr>
                        <td>{{this.email}}</td>
                        <td>{{this.level}}</td>
                        <td>{{this.message}}</td>
                        <td>{{this.str_date_time}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>